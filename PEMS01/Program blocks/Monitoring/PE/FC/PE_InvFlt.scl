	THEN
	    #wOut.%X5 := True;
	END_IF;
	
	IF #iInput = #i6
	THEN
	    #wOut.%X6 := True;
	END_IF;
	
	IF #iInput = #i7
	THEN
	    #wOut.%X7 := True;
	END_IF;
	
	IF #iInput = #i8
	THEN
	    #wOut.%X8 := True;
	END_IF;
	
	IF #iInput = #i9
	THEN
	    #wOut.%X9 := True;
	END_IF;
	
	IF #iInput = #i10
	THEN
	    #wOut.%X10 := True;
	END_IF;
	
	IF #iInput = #i11
	THEN
	    #wOut.%X11 := True;
	END_IF;
	
	IF #iInput = #i12
	THEN
	    #wOut.%X12 := True;
	END_IF;
	
	IF #iInput = #i13
	THEN
	    #wOut.%X13 := True;
	END_IF;
	
	IF #iInput = #i14
	THEN
	    #wOut.%X14 := True;
	END_IF;
	
	IF #iInput = #i15
	THEN
	    #wOut.%X15 := True;
	END_IF;
	
	IF #xReset
	THEN
	    #wOut := 0;
	END_IF;
	
END_FUNCTION

