FUNCTION "PE_W_Block3_old" : Void
{ S7_Optimized_Access := 'TRUE' }
VERSION : 0.1
   VAR_INPUT 
      In_Data : Real;
   END_VAR

   VAR_OUTPUT 
      Out_Data : Array[1..125] of Word;
   END_VAR


BEGIN
	
	REGION
	    #Out_Data[1] := INT_TO_WORD(REAL_TO_INT(#In_Data * 100.0));
	END_REGION
END_FUNCTION

